service fps_hal /vendor/bin/hw/android.hardware.biometrics.fingerprint@2.1-service.nubia_nx569j
    # "class hal" causes a race condition on some devices due to files created
    # in /data. As a workaround, postpone startup until later in boot once
    # /data is mounted.
    class late_start
    socket fpce stream 0666 system system
    user system
    group system uhid input
    system system
    disabled
